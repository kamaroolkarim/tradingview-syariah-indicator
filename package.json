{
  "name": "tradingview-syariah-indicator",
  "version": "1.4.3",
  "description": "Display small shariah indicator to stock list in tradingview(MY)",
  "private": true,
  "scripts": {
    "start": "web-ext run --start-url https://tradingview.com/symbols/MYX-k1/",
    "start:chrome": "web-ext run --target chromium --start-url https://tradingview.com/symbols/MYX-k1/",
    "update-data": "node update-data",
    "pre:build": "node prod_script.js",
    "build": "web-ext build --source-dir \"dist\" --overwrite-dest",
    "firefox:prod": "npx web-ext run --sourceDir=release/build",
    "firefox:dev": "npx web-ext run --sourceDir=build"
  },
  "engines": {
    "node": "12.18.3",
    "npm": "6.14.6"
  },
  "keywords": [
    "Syariah Stock in Malaysia",
    "Malaysia",
    "Stock",
    "Bursa Malaysia"
  ],
  "author": "Azriz Haziq <azrizhaziq@gmail.com> (https://azrizhaziq.github.io/portfolio)",
  "license": "ISC",
  "devDependencies": {
    "cli-progress": "^3.6.0",
    "csso": "^4.0.3",
    "eslint": "^6.8.0",
    "exceljs": "^4.1.1",
    "html-minifier": "^4.0.0",
    "lodash.merge": "^4.6.2",
    "ncp": "^2.0.0",
    "playwright": "^0.11.1",
    "rimraf": "^3.0.2",
    "simple-git": "^1.132.0",
    "terser": "^5.2.1",
    "web-ext": "^5.0.0",
    "webextension-polyfill": "^0.6.0"
  }
}
